{
    "name": "HiAnime",
    "version": "1.0.0",
    "author": {
        "name": "sobet",
        "website": "https://hianime.to/"
    },
    "iconURL": "https://hianime.to/favicon.ico",
    "stream": "HLS",
    "language": "EN",
    "extractor": "pattern-mp4",
    "module": [
        {
            "search": {
                "url": "https://hianime.to/search?keyword=",
                "parameter": "keyword",
                "documentSelector": "div.search-item div.result-item",
                "title": "h3.title",
                "image": {
                    "url": "img",
                    "attribute": "src"
                },
                "href": "a"
            },
            "featured": {
                "url": "https://hianime.to/recently-updated",
                "documentSelector": "div.featured div.item",
                "title": "a.title",
                "image": {
                    "url": "img",
                    "attribute": "src"
                },
                "href": "a"
            },
            "details": {
                "baseURL": "https://hianime.to",
                "aliases": {
                    "selector": "",
                    "attribute": ""
                },
                "synopsis": "div.description p",
                "airdate": "",
                "stars": ""
            },
            "episodes": {
                "selector": "div.episodes-container a",
                "order": "ordered",
                "pattern": ""
            }
        }
    ]
}
