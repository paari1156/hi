{
    "name": "HiAnime",
    "version": "1.0.0",
    "author": {
        "name": "paari",
        "website": "https://google.com"
    },
    "iconURL": "https://hianime.to/images/logo.png",
    "stream": "HLS",
    "language": "EN",
    "extractor": "normal",
    "module": [
        {
            "search": {
                "url": "https://hianime.to/search?keyword=",
                "parameter": "keyword",
                "documentSelector": "div.film_list-wrap div.flw-item",
                "title": "div.film-poster a",
                "image": {
                    "url": "div.film-poster img",
                    "attribute": "src"
                },
                "href": "div.film-poster a"
            },
            "featured": {
                "url": "https://hianime.to/home",
                "documentSelector": "div.film_list-wrap div.flw-item",
                "title": "div.film-poster a",
                "image": {
                    "url": "div.film-poster img",
                    "attribute": "src"
                },
                "href": "div.film-poster a"
            },
            "details": {
                "baseURL": "https://hianime.to",
                "aliases": {
                    "selector": "h2.film-name",
                    "attribute": "data-jname"
                },
                "synopsis": "div.film-description",
                "airdate": "div.meta-data span.item:contains(Released)",
                "stars": "div.meta-data span.item:contains(Rating)"
            },
            "episodes": {
                "selector": "div.ss-list a.ep-item",
                "order": "ordered",
                "pattern": "https://[\\w.-]+/[\\w.-]+/[\\w]{32}/master\\.m3u8"
            }
        }
    ]
}
